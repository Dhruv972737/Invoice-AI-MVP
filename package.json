{
  "name": "invoice-ai-platform",
  "version": "2.0.0",
  "description": "Complete Multi-Agent AI Invoice Processing Platform",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "start": "echo \"This project uses FastAPI for backend. Run backend with: cd backend/fastapi && source .venv/bin/activate && uvicorn main:app --reload --host 0.0.0.0 --port 10000\" && exit 1",
